
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Experience Update Page</h4>
<br />
<form method="post">
    <input type="text" name="ID" class="form-control" placeholder="ID" />
    <br />
    <input type="text" name="Baslik" class="form-control" placeholder="Title" />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Subtitle" />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Working Date" />
    <br />
    <textarea placeholder="Comment" name="Aciklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Update</button>
</form>

