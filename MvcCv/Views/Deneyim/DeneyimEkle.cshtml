
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Deneyim Güncelle Sayfası</h4>
<br />
<form method="post">
    <input type="text" name="ID" class="form-control" placeholder="ID" />
    <br />
    <input type="text" name="Baslik" class="form-control" placeholder="Basligi Yaziniz" />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlık giriniz" />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Çalışma Tarihi" />
    <br />
    <textarea placeholder="Açıklamanızı Giriniz" name="Aciklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Güncelle</button>
</form>

